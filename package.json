  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode prod",
    "local": "vite --mode local",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
