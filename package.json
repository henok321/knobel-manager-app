{
  "name": "knobel-manager-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.30.3",
  "engines": {
    "node": ">=24.0.0 <25.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "prod": "vite --mode production",
    "local": "vite --mode development",
    "api:gen": "openapi-ts",
    "build": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write --ignore-unknown .",
    "fix": "pnpm format && pnpm lint:fix",
    "test": "jest",
    "knip": "knip --strict",
    "knip:fix": "knip --strict --fix --format",
    "preview": "vite preview",
    "prepare": "husky",
    "clean": "rm -rf node_modules dist",
    "deploy": "pnpm build && firebase deploy --only hosting"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm eslint --fix",
      "pnpm prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "pnpm prettier --write"
    ]
  },
  "dependencies": {
    "@mantine/core": "^8.3.15",
    "@mantine/form": "^8.3.15",
    "@mantine/hooks": "^8.3.15",
    "@mantine/modals": "^8.3.15",
    "@mantine/notifications": "^8.3.15",
    "@reduxjs/toolkit": "^2.11.2",
    "@tabler/icons-react": "^3.37.1",
    "axios": "^1.13.5",
    "firebase": "^12.9.0",
    "i18next": "^25.8.13",
    "i18next-browser-languagedetector": "^8.2.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-i18next": "^16.5.4",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.13.1",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.3",
    "@eslint/markdown": "^7.5.1",
    "@hey-api/openapi-ts": "^0.93.0",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.14",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/react-redux": "^7.1.34",
    "@vitejs/plugin-react": "^5.1.4",
    "babel-jest": "^30.2.0",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.15.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.2",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-util": "^30.2.0",
    "knip": "^5.85.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.10",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vite": "^7.3.1",
    "vite-plugin-mkcert": "^1.17.10",
    "web-streams-polyfill": "^4.2.0",
    "whatwg-fetch": "^3.6.20"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.59.0"
  },
  "pnpm": {
    "overrides": {
      "minimatch": ">=10.2.1"
    }
  }
}
