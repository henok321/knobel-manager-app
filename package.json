{
  "name": "knobel-manager-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0 <25.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "prod": "vite --mode production",
    "local": "vite --mode development",
    "api:gen": "openapi-generator-cli generate -i https://api.knobel-manager.de/openapi.yaml -g typescript-axios -o src/generated --additional-properties=withSeparateModelsAndApi=true,modelPropertyNaming=original,supportsES6=true,apiPackage=apis,modelPackage=models",
    "build": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write --ignore-unknown .",
    "fix": "npm run format && npm run lint:fix",
    "test": "jest",
    "knip": "knip --strict",
    "knip:fix": "knip --strict --fix --format",
    "preview": "vite preview",
    "prepare": "husky",
    "clean": "rm -rf node_modules dist",
    "deploy": "npm run build && firebase deploy --only hosting"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@mantine/core": "^8.3.9",
    "@mantine/form": "^8.3.9",
    "@mantine/hooks": "^8.3.9",
    "@mantine/modals": "^8.3.9",
    "@mantine/notifications": "^8.3.9",
    "@reduxjs/toolkit": "^2.11.0",
    "@tabler/icons-react": "^3.35.0",
    "axios": "^1.13.2",
    "firebase": "^12.6.0",
    "i18next": "^25.7.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.3.5",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.6",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@eslint/markdown": "^7.5.1",
    "@openapitools/openapi-generator-cli": "^2.25.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-redux": "^7.1.34",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-jest": "^30.2.0",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.2.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "knip": "^5.71.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.3",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.7.3",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vite-plugin-mkcert": "^1.17.9",
    "web-streams-polyfill": "^4.2.0",
    "whatwg-fetch": "^3.6.20"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.53.3"
  }
}
