{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "ignore": [
    "coverage/**",
    "dist/**",
    "build/**",
    "src/generated/**",
    "src/test/**",
    "openapi-config.js",
    "src/api/emptyApi.ts",
    "scripts/**"
  ],
  "ignoreExportsUsedInFile": {
    "interface": true,
    "type": true
  },
  "ignoreDependencies": ["@rtk-query/codegen-openapi"],
  "jest": {
    "config": ["jest.config.js"],
    "entry": ["jest.setup.js"]
  }
}
